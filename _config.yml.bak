auto: true
markdown: rdiscount
pygments: true

var:
  archive_path: /archive.html
  categories_path : /categories.html
  tags_path : /tags.html
  rss_path: /atom.xml

production_url : http://mookrs.github.com # or your custom domain name
title : Mookrs

author : 
  name : Mookrs
  email : moookrs@gmail.com
  github : mookrs
  twitter : mookrs
  feedbuder : 

comments :
    provider : disqus
    disqus :
      short_name : mookrs
    livefyre :
      site_id : 
    intensedebate :
